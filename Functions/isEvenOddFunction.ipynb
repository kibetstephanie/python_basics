   "source": [
    "#Function to check if a number is Even or Odd\n",
    "def CheckEvenOdd(num): \n",
    "  if (num % 2 == 0): \n",
    "    print(num,\" is Even\")\n",
    "  else: \n",
    "    print(num,\" is Odd\")\n",
    "    \n",
    "#Calling the function   \n",
    "CheckEvenOdd(11) \n",
    "CheckEvenOdd(22) \n",
    "CheckEvenOdd(33)\n",
    "CheckEvenOdd(44)"
   ]
